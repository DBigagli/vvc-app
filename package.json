{
  "$schema": "./node_modules/ng-packagr/ng-package.schema.json",
  "name": "@vivocha/client-interaction-layout",
  "version": "6.0.0",
  "license": "SEE LICENSE IN LICENSE",
  "scripts": {
    "clean": "[ ! -d ./dist ] || rm -rf dist/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prebuild": "npm run clean",
    "build": "ng-packagr -p package.json",
    "precommit": "npm run build",
    "version": "jsonref -f ./package.json /version",
    "build:all": "npm run clean:all && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/vivocha/client-interaction-layout"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "homepage": "https://bitbucket.org/vivocha/client-interaction-layout#readme",
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts",
      "licensePath": "LICENSE"
    }
  },
  "peerDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@ngx-translate/core": "9.0.2",
    "@ngx-translate/http-loader": "2.0.1",
    "@vivocha/public-entities": "6.3.0",
    "ngx-auto-scroll": "1.1.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@ngx-translate/core": "9.0.2",
    "@ngx-translate/http-loader": "2.0.1",
    "ngx-auto-scroll": "1.1.0",
    "@angular/cli": "~1.7.1",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "jsonref-cli": "^1.0.0",
    "ng-packagr": "2.4.2",
    "typescript": "2.4.0"
  }
}
