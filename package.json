{
  "name": "@vivocha/client-interaction",
  "version": "6.4.43",
  "description": "Vivocha Interaction Application",
  "scripts": {
    "prepare": "npm run build:prod",
    "clean": "[ ! -d ./dist/assets ] || rm -rf dist/assets/* && [ ! -d ./dist/ie ] || rm -rf dist/ie/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "cpmain": "cp ./src/main.scss ./dist/main.scss && cp ./src/manifest.json ./dist/manifest.json && cp ./src/strings.json ./dist/strings.json",
    "dist": "[ -d ./dist ] || mkdir ./dist",
    "build:app": "ng build @vivocha/client-interaction --delete-output-path=false",
    "build:app:ie": "ng build @vivocha/client-interaction-ie --delete-output-path=false && npm run babel:ie",
    "prebuild": "npm run clean",
    "build": "npm run build:app && npm run build:app:ie",
    "postbuild": "npm run dist && cp ./src/main.html ./dist/main.html && npm run cpmain && node bannerize",
    "build:app:prod": "ng build @vivocha/client-interaction --prod --output-hashing none",
    "build:app:ie:prod": "ng build @vivocha/client-interaction-ie --prod --output-hashing none && npm run babel:ie",
    "prebuild:prod": "npm run prebuild",
    "build:prod": "npm run build:app:prod && npm run build:app:ie:prod",
    "postbuild:prod": "npm run dist && cp ./src/main.prod.html ./dist/main.html && npm run cpmain && node bannerize",
    "build:app:watch": "ng build @vivocha/client-interaction --delete-output-path=false --preserve-symlinks --watch",
    "build:app:ie:watch": "ng build @vivocha/client-interaction-ie --delete-output-path=false --preserve-symlinks --watch",
    "prebuild:watch": "npm run prebuild && npm run dist && cp ./src/main.watch.html ./dist/main.html && npm run cpmain && echo \"\\033[0;31mPLEASE NOTE THAT build:watch may not work with IE11\\033[0m\"",
    "build:watch": "npm run build:app:watch",
    "prebabel:ie": "mkdirp dist/ie",
    "babel:ie": "babel --presets=env dist/ie --out-dir dist/assets/ie",
    "babel:ie:watch": "npm run babel:ie -- --watch",
    "postbabel:ie": "rm -fr dist/ie",
    "build:all": "npm run clean:all && npm i && npm run build:prod",
    "precommit": "npm run build",
    "__precommit": "npm run build && npm run cover && npm run check-coverage",
    "version": "which jsonref > /dev/null || npm i jsonref-cli && jsonref -f ./package.json /version",
    "ng": "ng",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:debug": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/vivocha/client-interaction.git"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://bitbucket.org/vivocha/client-interaction#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.14",
    "@angular/cli": "^8.3.14",
    "@angular/compiler": "^8.2.12",
    "@angular/compiler-cli": "^8.2.12",
    "ng-packagr": "^5.7.0",
    "typescript": "~3.5.0",
    "@angular-devkit/build-ng-packagr": "^0.803.14",
    "@angular/language-service": "8.2.12",
    "@types/jasmine": "3.4.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.11.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "classlist.js": "^1.1.20150312",
    "codelyzer": "5.2.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "jsonref-cli": "^1.0.0",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.2",
    "ts-node": "8.4.1",
    "tsickle": ">=0.37.0",
    "tslib": "^1.10.0",
    "tslint": "5.20.0"
  },
  "dependencies": {
    "@angular/common": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/forms": "^8.2.12",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@ngrx/store": "^8.4.0",
    "@ngrx/store-devtools": "^8.4.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@vivocha/client-interaction-core": "^6.4.36",
    "@vivocha/client-interaction-layout": "^6.4.20",
    "@vivocha/public-entities": "^7.0.10",
    "autolinker": "^1.6.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "3.3.3",
    "rxjs": "^6.5.3",
    "zone.js": "0.10.2"
  }
}
