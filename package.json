{
  "name": "@vivocha/client-interaction",
  "version": "6.4.1",
  "description": "Vivocha Interaction Application",
  "scripts": {
    "clean": "[ ! -d ./dist/assets ] || rm -rf dist/assets/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prebuild": "npm run clean",
    "cpmain": "cp src/main.scss dist/main.scss && cp src/manifest.json dist/manifest.json && cp src/strings.json dist/strings.json",
    "build": "npm run build:index && ng build vvc-widget --delete-output-path=false",
    "build:ie": "npm run build:index && ng build vvc-widget-ie --delete-output-path=false && npm run babel:ie",
    "build:index": "cp src/main.html dist/main.html && npm run cpmain",
    "precommit": "npm run build",
    "_precommit": "npm run build && npm run cover && npm run check-coverage",
    "build:prod": "npm run build:index:prod && ng build vvc-widget --prod --output-hashing none",
    "build:ie:prod": "npm run build:index:prod && ng build vvc-widget-ie --prod --output-hashing none && npm run babel:ie",
    "build:index:prod": "cp src/main.prod.html dist/main.html && npm run cpmain",
    "babel:ie": "babel --presets=env dist/assets/ie --out-dir dist/assets/ie",
    "version": "which jsonref > /dev/null || npm i jsonref-cli && jsonref -f ./package.json /version",
    "build:all": "npm run clean:all && npm install && npm run build:prod && npm run build:ie:prod",
    "build:watch": "ng build --delete-output-path=false --preserve-symlinks --watch",
    "ng": "ng",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:debug": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/vivocha/client-interaction.git"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://bitbucket.org/vivocha/client-interaction#readme",
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular-devkit/build-ng-packagr": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler": "^6.0.9",
    "@angular/compiler-cli": "^6.0.9",
    "@angular/language-service": "^6.0.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonref-cli": "^1.0.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.3",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "@angular/common": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@vivocha/client-interaction-core": "^6.4.0",
    "@vivocha/client-interaction-layout": "^6.4.0",
    "@vivocha/public-entities": "^6.4.15",
    "autolinker": "^1.6.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  }
}
