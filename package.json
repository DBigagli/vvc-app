  "description": "Vivocha Interaction Application",
  "scripts": {
    "clean": "[ ! -d ./dist ] || rm -rf dist/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prebuild": "npm run clean",
    "build": "ng build --delete-output-path=false",
    "precommit": "npm run build",
    "_precommit": "npm run build && npm run cover && npm run check-coverage",
    "build:prod": "ng build",
    "_build:prod": "ng build --prod",
    "version": "jsonref -f ./package.json /version",
    "build:all": "npm run clean:all && npm install && npm run build:prod",
    "build:watch": "ng build --delete-output-path=false --watch",
    "ng": "ng",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:debug": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "__scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
