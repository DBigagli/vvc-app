  "scripts": {
    "clean": "[ ! -d ./dist ] || rm -rf dist/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prebuild": "npm run clean",
    "build": "ng-packagr -p ng-package.json",
    "precommit": "npm run build",
    "version": "which jsonref > /dev/null || npm i jsonref-cli && jsonref -f ./package.json /version",
    "fixdeps": "[ ! -f ./dist/package.json ] || ./fixdeps.js",
    "build:all": "npm run clean:all && npm install && npm run build && npm run fixdeps"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/vivocha/client-interaction-layout"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "homepage": "https://bitbucket.org/vivocha/client-interaction-layout#readme",
  "peerDependencies": {
    "@angular/common": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@ngx-translate/core": "^10.0.2",
    "@vivocha/public-entities": "^6.4.15"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler": "^6.0.9",
    "@angular/compiler-cli": "^6.0.9",
    "jsonref-cli": "^1.0.0",
    "ng-packagr": "^3.0.3",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "@angular/common": "^6.0.9",
    "@angular/core": "^6.0.9",
    "@angular/forms": "^6.0.9",
    "@angular/platform-browser": "^6.0.9",
    "@angular/platform-browser-dynamic": "^6.0.9",
    "@ngx-translate/core": "^10.0.2",
    "@vivocha/public-entities": "^6.4.15"
  }
}
