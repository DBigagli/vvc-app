{
  "name": "@vivocha/client-interaction",
  "version": "6.2.1",
  "description": "Vivocha Interaction Application",
  "scripts": {
    "clean": "[ ! -d ./dist/assets ] || rm -rf dist/assets/*",
    "clean:all": "npm run clean && rm -rf node_modules",
    "prebuild": "npm run clean",
    "build": "npm run build:index && ng build vvc-widget --delete-output-path=false",
    "build:ie": "npm run build:index && ng build vvc-widget-ie --delete-output-path=false && npm run babel:ie",
    "build:index": "cp src/main.html dist/main.html",
    "precommit": "npm run build",
    "_precommit": "npm run build && npm run cover && npm run check-coverage",
    "_build:prod": "ng build",
    "build:prod": "npm run build:index:prod && ng build vvc-widget --prod --output-hashing none",
    "build:ie:prod": "npm run build:index:prod && ng build vvc-widget-ie --prod --output-hashing none && npm run babel:ie",
    "build:index:prod": "cp src/main.prod.html dist/main.html",
    "babel:ie": "babel --presets=env dist/assets/ie --out-dir dist/assets/ie",
    "version": "jsonref -f ./package.json /version",
    "build:all": "npm run clean:all && npm install && npm run build:prod && npm run build:ie:prod",
    "build:watch": "ng build --delete-output-path=false --preserve-symlinks --watch",
    "push": "cd dist && vvc widget push && cd -",
    "push:global": "cd dist && vvc widget push -g && cd -",
    "push:all": "npm run build:all && cd dist && vvc widget push && cd -",
    "push:all:global": "npm run build:all && cd dist && vvc widget push -g && cd -",
    "ng": "ng",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage",
    "test:debug": "ng test -sm=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "__scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "coverage": "ng test --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/vivocha/client-foundry-widget.git"
  },
  "author": "Vivocha Dev Team <dev@vivocha.com> (http://www.vivocha.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://bitbucket.org/vivocha/client-foundry-widget#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@angular/common": "^5.1.2",
    "@angular/core": "^5.1.2",
    "@angular/forms": "^5.1.2",
    "@angular/http": "^5.1.2",
    "@angular/platform-browser": "^5.1.2",
    "@angular/platform-browser-dynamic": "^5.1.2",
    "@angular/router": "^5.1.2",
    "@ngrx/store": "5.1.0",
    "@ngx-translate/core": "9.0.2",
    "@ngx-translate/http-loader": "2.0.1",
    "@vivocha/client-interaction-core": "6.2.0",
    "@vivocha/client-interaction-layout": "6.0.1",
    "core-js": "^2.5.7",
    "ngx-auto-scroll": "1.1.0",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/core": "^0.2.0",
    "@angular/cli": "^6.0.8",
    "@angular/compiler": "^5.1.2",
    "@angular/compiler-cli": "^5.1.2",
    "@ngrx/store-devtools": "5.1.0",
    "@types/jasmine": "2.8.2",
    "@types/node": "~8.5.2",
    "@vivocha/public-entities": "^6.3.0",
    "codelyzer": "~4.0.2",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsonref-cli": "^1.0.0",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.2.2",
    "ts-node": "~4.0.2",
    "tslint": "~5.8.0",
    "typescript": "~2.6.2",
    "@angular-devkit/build-angular": "~0.6.8",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
